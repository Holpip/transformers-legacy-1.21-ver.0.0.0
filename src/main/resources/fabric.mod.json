		"fabric-datagen": [
			"net.holpip.transformerslegacy.TransformersLegacyDataGenerator"
		]
	},
	"mixins": [
		"transformerslegacy.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.2",
		"minecraft": "~1.21",
